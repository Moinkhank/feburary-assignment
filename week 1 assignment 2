                             //Check if a year is leap year or not
                             package com.company;
import java.util.Scanner;
public class Main {



    public static void main(String[] args) {

        Scanner sc =new Scanner(System.in);
        int year = sc.nextInt();
        if(year % 4==0)
        {
            if(year % 100==0)
            {
                if(year % 400==0)
                {
                    System.out.println("leap year");
                }
               
            }


        }
        else{
            System.out.println("not leap year");
        }

    }
}
             // Write algorithm to print Sum of even and odd digits, considering 10 numbers are taken from user
                 public static void main(String[] args) {

       int num = 10;
       int sum1 = 0,sum2 = 0;
       for(int i =1;i<=num;i++)
       {
           if(i%2==0)
           {
              sum1 += i;
           }
           else if(i%2 != 0)
           {
               sum2 += i;
           }
       }
        System.out.println("sum of even no "+sum1);
        System.out.println("sum of odd no "+sum2);
    }
}
